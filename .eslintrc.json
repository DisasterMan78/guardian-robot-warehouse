{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "one-var": 0,
        "indent": [
        "error", 2,
        {
            "VariableDeclarator": 3,
            "SwitchCase": 1
        }]
    }
}